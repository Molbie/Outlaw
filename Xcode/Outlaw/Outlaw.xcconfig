MACOS_VALID_ARCH = i386 x86_64
SIMULATOR_VALID_ARCH = $(MACOS_VALID_ARCH)
IOS_VALID_ARCH = armv7 armv7s arm64
TVOS_VALID_ARCH = arm64
WATCHOS_VALID_ARCH = armv7k

VALID_ARCHS[sdk=macosx] = $(MACOS_VALID_ARCH)
VALID_ARCHS[sdk=iphonesimulator] = $(SIMULATOR_VALID_ARCH)
VALID_ARCHS[sdk=iphoneos] = $(IOS_VALID_ARCH)
VALID_ARCHS[sdk=appletvsimulator] = $(SIMULATOR_VALID_ARCH)
VALID_ARCHS[sdk=appletvos] = $(TVOS_VALID_ARCH)
VALID_ARCHS[sdk=watchsimulator] = $(SIMULATOR_VALID_ARCH)
VALID_ARCHS[sdk=watchos] = $(WATCHOS_VALID_ARCH)
